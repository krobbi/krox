// A parenthetical expression node of an abstract syntax tree.
class ParentheticalExprASTNode < ASTNode {
	// Initialize the parenthetical expression AST node's expression.
	init(expression){
		super.init();
		
		// The parenthetical expression AST node's expression.
		this.expression = expression;
	}
	
	// Accept an AST visitor and return a value.
	accept_visitor(visitor){
		return visitor.visit_parenthetical_expr(this);
	}
}
